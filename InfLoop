using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace inflloop
{
    class InfLoop
    {
        static void Main(string[] args)
        {
            for (var i = 1; i < 2; i--)
            {
                Console.WriteLine("Fuck You");
                i++;
            }
        }
    }
}
